{
    "craydateProjectRootCrate": "windup-project",
    "rust-analyzer.linkedProjects": [
        "windup-project/Cargo.toml",
    ],

    "craydateHostTarget": "x86_64-pc-windows-msvc",
    "craydateTargetPlatform": "windows",
    "linux": {
        "craydateHostTarget": "x86_64-unknown-linux-gnu",  // Is this right?
        "craydateTargetPlatform": "linux",
    },
    "osx": {
        "craydateHostTarget": "x86_64-apple-darwin",  // What about arm? Get it from a command?
        "craydateTargetPlatform": "osx",
    },
    "rust-analyzer.cargo.features": [
        "bins"
    ],
    "rust-analyzer.checkOnSave.allTargets": false,
 
    "[rust]": {
        "editor.rulers": [
            100
        ],
    }
}